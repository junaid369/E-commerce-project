										{{/each}}
								</ul>
							</div>
						</aside>




						<div class="desc">
							<h2><a href="#">Shop Dress Collection</a></h2>
						</div>
					</div>
				</div>
				<div class="col-sm-6 text-center">
					<div class="featured">

						<aside id="colorlib-hero">
							<div class="flexslider">
								<ul class="slides">


									</li>
									{{#each banner}}
									<li style="background-image: url(/banner-images/{{this._id}}.c.jpg);">
										{{/each}}
									<li style="background-image: url(images/img_bg_1.jpg);"></li>
								</ul>
							</div>
						</aside>
						<div class="desc">
							<h2><a href="#">Shop Women's footware Collection</a></h2>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="colorlib-product">
		<div class="container">
			<div class="row" style="margin-top: -170px;" >
				<div class="col-sm-8 offset-sm-2 text-center colorlib-heading">
					<h2>Best Sellers</h2>
				</div>
			</div>





			<div class="container">
				<div class="row" style="">
					{{#each products}}
					<div class="col-lg-3 p-4">

						<div class="product-grid">

							<div class="product-image">
								<a href="/singleproduct/{{this._id}}" class="image">
									<img class="pic-1 " src="product-images/{{this._id}}.a.jpg">
									<img class="pic-2" src="product-images/{{this._id}}.b.jpg">
								</a>



								<a style="background: none;"  onclick="addtowishlist('{{this._id}}')" class="product-like-icon"
									data-tip="Add to Wishlist">
									<i class="far fa-heart"></i>
								</a>
								<ul class="product-links">

									<li><a   onclick="addtocart('{{this._id}}')" style=""><i
												class="fas fa-shopping-cart"></i></a></li>
									<li><a href="/singleproduct/{{this._id}}"><i class="fa fa-eye"></i></a></li>
								</ul>
							</div>


							{{#if this.proOffer}}
							<div class="product-content">
								<h3 class="title"><a href="#">{{this.name}}</a></h3>
								<div class="price">{{this.PPrice}}

									<span class="" style="color: brown;"><s>RS:{{this.actualPrice}}</s></span>
									<a href="/buyNow/{{this._id}}" class="ml-3">Buy Now</a>
								</div>
							</div>
							{{else}}
							<div class="product-content">
								<h3 class="title"><a href="#">{{this.name}}</a></h3>
								<div class="price">{{this.PPrice}}
									<a href="/buyNow/{{this._id}}" class="ml-5">Buy Now</a>
								</div>
							</div>
							{{/if}}



						</div>
					</div>

					{{/each}}
				</div>
			</div>

			<div class="row">
				<div class="col-md-12 text-center">
					<p><a href="/women" class="btn btn-primary btn-lg" style="background-color: #1042F1;">Shop All
							Products</a></p>
				</div>
			</div>
		</div>
	</div>

	<div class="colorlib-partner">
		<div class="container">
