<div class="container" style="margin-top: 10rem;">

  <div>
    <h4 class="text-center" style="margin-bottom: 40px;">YOUR ORDERS</h4>
  </div>
    {{#each products}}
  <div class="card" style="width: 18rem;">
  <img class="card-img-top" src="/product-images/{{this.product._id}}.a.jpg" alt="Card image cap">
  <div class="card-body">
    <h6 class="card-title">Name:{{this.product.name}}</h6>
    <h6 class="card-title">Brand:{{this.product.brand}}</h6>
    <h6 class="card-title">Size:{{this.product.size}}</h6>
    <h6 class="card-title">RS:{{this.product.PPrice}}</h6>


{{!-- 
    <p class="card-text" style="margin-top: -50px;">{{this.product.description}}<br>
    {{this.product.brand}}<br>
   <h6> Category:<span> {{this.product.Mcategory}}</span></h6>
   {{this.product.cat}}{{this.product.size}}</p>
      <h6 class="card-title">Rs:{{this.product.PPrice}}</h6> --}}
    
  </div>
</div>
{{/each}}
</div>
